{
  "name": "asian-restaurant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "-------- Prepair Database & Stripe Event localhost forwarding --------": "",
    "db:generate-types": "dotenv -e .env.development.local -- prisma generate",
    "db:push:development": "dotenv -e .env.development.local -- npx prisma db push --accept-data-loss",
    "db:dev": "npm run db:push:development && npm run db:generate-types",
    "-------- Run --------": "",
    "dev": "next dev --turbopack",
    "-------- Run Preview (production build still with development environment) --------": "",
    "preview": "dotenv -e .env.development.local next build && dotenv -e .env.development.local next start",
    "-------- Production --------": "",
    "build": "npm run db:dev && next build",
    "start": "next start",
    "-------- Other --------": "",
    "lint": "eslint ."
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@hookform/resolvers": "^3.9.1",
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@vercel/blob": "^0.27.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "dotenv": "^16.4.7",
    "lucide-react": "^0.468.0",
    "next": "15.1.2",
    "next-auth": "^5.0.0-beta.25",
    "prisma": "^6.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-comment-length": "^2.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-n": "^17.15.0",
    "eslint-plugin-perfectionist": "^4.4.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
